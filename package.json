{
  "name": "@arch-inc/fabricjs-psbrush",
  "version": "0.0.15",
  "description": "A lightweight pressure-sensitive brush implementation for Fabric.js",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "clean": "del-cli dist",
    "prettify": "prettier --write ./**/*.{ts,tsx}",
    "dev-pages": "next ./docs",
    "build-pages": "next build ./docs",
    "start-pages": "next start ./docs",
    "export-pages": "next export ./docs",
    "build-docs": "typedoc --mode library --excludeExternals --excludeNotExported --excludePrivate --excludeProtected --out ./docs/out/api ./lib/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arch-inc/fabricjs-psbrush.git"
  },
  "keywords": [
    "canvas",
    "graphic",
    "graphics",
    "SVG",
    "node-canvas",
    "fabric"
  ],
  "author": "Jun Kato <i@junkato.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arch-inc/fabricjs-psbrush/issues"
  },
  "homepage": "https://github.com/arch-inc/fabricjs-psbrush#readme",
  "dependencies": {
    "fabric": "^4.0.0-beta.7"
  },
  "devDependencies": {
    "@types/fabric": "^3.6.2",
    "@types/react": "^17.0.2",
    "del-cli": "^3.0.1",
    "next": "^10.0.6",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "typedoc": "^0.17.0-3",
    "typescript": "^4.1.5"
  }
}
